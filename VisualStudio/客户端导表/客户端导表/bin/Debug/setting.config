<setting>
  <pathSettings>
    <!--Excel 路径 导出的txt路径-->
    <ExcelPath>D:/temp/</ExcelPath>
    <TxtPath>D:/temp/config/</TxtPath>
    <CheckData>false</CheckData>
    <PackageMode>true</PackageMode>
    <Compress>true</Compress>

    <!--总路径， Base路径, Class路径， 附加内容, 命名空间-->
    <CodePath>D:/temp/game/</CodePath>
    <CodeBasePath>D:/temp/game_config/src/</CodeBasePath>
    <CodeClassPath>D:/temp/game/tables/tableClass/</CodeClassPath>
    <CodeAppend></CodeAppend>
    <CodeNamespace>game_config</CodeNamespace>

  </pathSettings>

  <excelSettings>
    <!--H5专用表-->
    <add key="物品表.xlsx" value="ItemCFG"/>
    <add key="邮件表.xlsx" value="MailCFG"/>
  </excelSettings>
</setting>